{
  "dashboard": {
    "title": "WekaDocs GraphRAG MCP - Query Performance",
    "tags": ["wekadocs", "mcp", "query", "performance"],
    "timezone": "utc",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Cypher Query Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(cypher_query_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 - {{template_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cypher_query_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 - {{template_name}}",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(cypher_query_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 - {{template_name}}",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "title": "Cypher Query Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(cypher_queries_total{status='success'}[5m]) / rate(cypher_queries_total[5m])",
            "legendFormat": "{{template_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "success rate", "format": "percentunit", "min": 0, "max": 1},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Cypher Validation Failures",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(cypher_validation_failures_total[5m])",
            "legendFormat": "{{reason}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "failures/sec", "format": "short"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Vector Search Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "{{store}} (P95)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Hybrid Search Latency (P50, P95, P99)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(hybrid_search_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(hybrid_search_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(hybrid_search_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.5], "type": "gt"},
              "query": {"params": ["B", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "frequency": "60s",
          "handler": 1,
          "name": "Hybrid Search P95 Latency Alert",
          "message": "Hybrid search P95 latency exceeded 500ms SLO"
        }
      },
      {
        "id": 6,
        "title": "Graph Expansion Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(graph_expansion_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "seconds", "format": "s"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Query Distribution by Template",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "sum by (template_name) (rate(cypher_queries_total[5m]))",
            "legendFormat": "{{template_name}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Slowest Query Templates (Top 5)",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "topk(5, histogram_quantile(0.95, rate(cypher_query_duration_seconds_bucket[5m])))",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ]
      }
    ]
  }
}
